{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConn": "server=(localdb)\\mssqllocaldb;database=configureEFCoreDb;Trusted_Connection=True;Encrypt=false"
  },
  "DatabaseOptions": {
    "MaxRetryCount": 5,
    "MaxRetryDelay": 30,
    "ErrorNumbersToAdd": null,
    "CommandTimeout": 30,
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  }
}
